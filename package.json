{
  "version": "0.0.4",
  "name": "bloombot",
  "main": "app/client/index.js",
  "description": "ⒸBloomBot (md) by Magneum™ is a whatsapp user-bot with automation, moderation, music, games and more of 200+ commands!",
  "scripts": {
    "local": "python3 app/python",
    "dev": "supervisor -w . boot.js",
    "replace": "node app/utils/replace.js",
    "ffdev": "nodemon app/lib/pfmpeg.js",
    "ffstart": "forever app/lib/pfmpeg.js",
    "dbclean": "node app/client/purgepg.js",
    "test": "supervisor -w . app/test/index.js",
    "magdev": "concurrently --kill-others \"yarn dev\" \"yarn ffdev\"",
    "build": "rm -rf yarn.lock && rm -rf node_modules && yarn install",
    "chatkeytart": "concurrently --kill-others \"yarn start\" \"yarn ffstart\"",
    "start": "forever --minUptime=1000ms --spinSleepTime=1000ms boot.js --color",
    "sync": "git init --initial-branch=magneum && git fetch origin magneum && git reset --hard origin/magneum"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magneum/bloombot.git"
  },
  "keywords": [
    "bot",
    "music",
    "games",
    "whatsapp",
    "moderation",
    "automation"
  ],
  "author": "Magneum",
  "license": "MIT",
  "engines": {
    "node": ">= 16",
    "yarn": ">= 1.22.19",
    "npm": "❌Error: This project requires you to use 'Yarn' package manager!"
  },
  "config": {
    "libPort": 8080,
    "logLevel": "info"
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": false,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "private": false,
